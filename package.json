{
  "name": "planexplor-backend",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node dist/app.js",
    "dev": "tsx watch src/app.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "prestart": "npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "express-async-errors": "^3.1.1",
    "pino": "^8.17.3",
    "pino-http": "^9.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.10.0"
  }
}
